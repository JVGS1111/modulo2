                    "Category"
                ],
                "summary": "Create a category",
                "description": "Create a new categorye",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "description": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                    "name": "Category test",
                                    "description": "Category description"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Category created"
                    },
                    "500": {
                        "description": "Category already existis"
                    }
